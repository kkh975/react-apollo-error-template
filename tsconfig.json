{
  "compilerOptions": {
    "experimentalDecorators": true,
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "target": "es6",
    "lib": ["DOM", "ES2015", "ES2016", "ES2017", "ES2018", "ES2019", "ES2020", "ES2021", "ES2022", "ESNext"],
    "noImplicitThis": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "baseUrl": "."
  }
}
